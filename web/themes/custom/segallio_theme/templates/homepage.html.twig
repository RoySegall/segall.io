<div class="container">
  <div class="section">

    <div class="row">
      <div class="col s12 center">
        <h3><i class="mdi-content-send brown-text"></i></h3>
        <h4>See what i'm doing on the web</h4>
      </div>
    </div>

  </div>

  {% verbatim %}
    <section id="timeline">
      <div class="center social-buttons">
        <i class="fab fa-facebook-square"></i>
        <i class="fab fa-twitter-square"></i>
        <i class="fab fa-instagram"></i>
        <i class="fab fa-github"></i>
      </div>

      <div class="demo-card-wrapper">
        <div v-for="(post, index) in posts" v-bind="{'class': post.className, 'item' : post.entity_type, 'id': post.id}">
          <div class="head">
				    <div class="number-box">
					    <span><div v-social-icon:icon="post.entity_type"></div></span>
				    </div>
				    <h2>Go to post</h2>
			    </div>
			    <div class="body">
			      {{post.name}}

            <img v-if="post.assets" v-bind="{'src': post.assets}" class="img-responsive" />
			    </div>
      </div>
    </section>
  {% endverbatim %}

</div>
