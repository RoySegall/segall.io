body{font-size:16px;padding-top:1em;font-family:muli,sans-serif}.header img{max-width:50%}.header h2{font-family:poiret one,cursive;padding-top:1em;padding-bottom:.5em;color:#6acff1;text-transform:uppercase;letter-spacing:.25em}.header hr{background:#f7b75e;width:75%}.body-wrapper{padding:1em;border-radius:.25em}.latest-projects,.latest-photos,.who-am-i,.professional-info{padding-top:2em}.latest-projects h3,.latest-photos h3,.who-am-i h3,.professional-info h3{font-size:2em;font-family:coiny,cursive;color:#6acff1}.who-am-i .profile-pic{border:solid 1px #f7b75e}.who-am-i ul{padding:0;padding-top:.5em}.who-am-i ul li{display:inline;padding-right:1em}.who-am-i ul li i{font-size:1.5em;color:#0090c1;-webkit-transition:color .5s;transition:color .5s}.who-am-i ul li i:hover{color:#f7b75e}.who-am-i .intro{padding-top:.25em;font-size:1.25em}.professional-info .wrapper{padding-top:1em}.professional-info .wrapper .super-power{padding-bottom:.5em}.professional-info .wrapper .super-power .full-star{color:#f7b75e}.professional-info .wrapper .super-power .empty-star{color:#6acff1}.professional-info .wrapper .job{border-bottom:solid 1px #f7b75e;margin-bottom:1.25em}.professional-info .wrapper .job span{font-size:1.25em}